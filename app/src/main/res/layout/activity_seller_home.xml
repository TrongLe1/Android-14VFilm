<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".sellerActivity.SellerHomeActivity">

    <FrameLayout
        android:id="@+id/flSellerHomeActivity"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />


    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/black"
        />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bnvSellerHomeActivity"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:itemTextColor="@color/black"
        app:itemIconTint="@color/black"
        app:itemTextAppearanceActive="@color/black"
        android:padding="2dp"
        android:background="@color/seller_menu"
        app:menu="@menu/seller_home_menu"
        />


</LinearLayout>